#include "holberton.h"
/**
* print_alphabet - check the code for Holberton School students.
*
* Return: Always 0.
*/
void print_alphabet(void)
{
char  abc;

For (abc ='a';gcc -Wall -pedantic -Werror -Wextra _putchar.c 1-main.c 1-alphabet.c -o 1-alphabet  <= 122; alfabet++)
{
_putchar (alfabet);
}
_putchar (10);
}
